# Config file token list, to be
# processed by gentoks.py

# standard symbols

*e      :   T_END
*s      :   T_STRING
*i      :   T_IDENT
*n      :   T_INT
*f      :   T_FLOAT

# characters, which return the token immediately and do not
# require trailing space

*c{     :   T_OCURLY
*c}     :   T_CCURLY
*c(     :   T_OPREN
*c)     :   T_CPREN
*c,     :   T_COMMA
*c<     :   T_LT


# words

audio   :   T_AUDIO
sample   :   T_SAMPLE
speech   :   T_SPEECH

false   :   T_FALSE
true    :   T_TRUE
bool    :   T_BOOL
integer :   T_INTEGER
else    :   T_ELSE
var     :   T_VAR
expr    :   T_EXPR
window  :   T_WINDOW
frame   :   T_FRAME
linked  :   T_LINKED
float   :   T_NAMEFLOAT
pos     :   T_POS
centred :   T_CENTRED
centered :   T_CENTERED
